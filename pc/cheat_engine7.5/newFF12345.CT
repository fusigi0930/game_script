aobscanregion(drop_mod, Last.Battle.BattleItemDropPlug.Lot, Last.Battle.BattleItemDropPlug.Lot+200, 8B CB 41 8D 50 64)
aobscanregion(rate_mod, Last.Battle.BattleItemDropPlug.Lot, Last.Battle.BattleItemDropPlug.Lot+300, 33 D2 48 8B CB e8 )
aobscanregion(lot_mod, Last.Battle.BattleItemDropPlug.LotGilIndividualDropItem, Last.Battle.BattleItemDropPlug.LotGilIndividualDropItem+200, 40 55 48 83 EC  )

alloc(drop_rate_99_with_weight_mod, $1024, drop_mod)

label(drop_setup_data)
label(drop_mod_code)
label(drop_mod_return)
label(rate_mod_code)
label(rate_mod_return)
label(lot_mod_code)
label(lot_mod_return)

label(commonPreset)
label(common)
label(done)
label(oriDrop)


registersymbol(drop_mod)
registersymbol(rate_mod)
registersymbol(lot_mod)
registersymbol(drop_rate_99_with_weight_mod)
registersymbol(drop_setup_data)
registersymbol(drop_mod_code)
registersymbol(rate_mod_code)
registersymbol(lot_mod_code)


drop_rate_99_with_weight_mod:
  readmem(drop_mod,16)
  readmem(rate_mod,16)
  readmem(lot_mod,16)

drop_mod_code:
commonPreset:
  mov rdx, drop_setup_data
  cmp byte ptr[rdx],0
  jne @f
  mov byte ptr[rdx+00],1 /// 1=enabled, other&gt;0 disabled, 0 will reset to default
  mov byte ptr[rdx+01],1 /// 1= use common added weight value
  mov byte ptr[rdx+02],1 /// 1= use added weight skew to higher(rarer?) in list
  mov byte ptr[rdx+03],1 /// 1= drop all, loop x8
  mov dword ptr[rdx+04],#1 /// common added weight, slightly up for skew default
  mov dword ptr[rdx+08],#32 /// skew highest, result 32/16/8/4/2/1/0... add to non zero original weight
  //mov dword ptr[rdx+0c],(float)2.0 /// div this for skew weight in each step
  mov dword ptr[rdx+10],#8 /// number of times to loop -&gt; defaults to 8
@@:
  ret
  aligndq

rate_mod_code:
  call commonPreset
  cmp byte ptr[rdx+0],1
  je @f
  readmem(rate_mod,5)
  jmp rate_mod_return
@@:
  push rsi
  push rdi
  push rbp
  xor rsi,rsi
  xor rdi,rdi
  cmp byte ptr [rdx+01], 1
  jne @f
  mov esi,[rdx+04]
@@:
  cmp byte ptr [rdx+02], 1
  jne @f
  mov edi,[rdx+08]
@@:
  xor rcx,rcx
  lea rbp,[rbx+3c] /// last weight
  call common
  lea rbp,[rbx+38] /// 2nd last weight, etc
  call common
  lea rbp,[rbx+34] ///
  call common
  lea rbp,[rbx+30] ///
  call common
  lea rbp,[rbx+2c] ///
  call common
  lea rbp,[rbx+28] ///
  call common
  lea rbp,[rbx+24] /// 2nd weight
  call common
  lea rbp,[rbx+20] /// 1st weight
  call common
  pop rbp
  pop rdi
  pop rsi
  readmem(rate_mod,5)
  jmp  rate_mod_return
common:
  mov eax, [rbp]
  mov [rcx*4+rdx+30], eax
  ///mov eax, [rbp]
  test eax, eax
  jle done /// don't change zero/negative weight
  cmp ecx, [rdx+0c]
  jne @f
  add eax, #1000
@@:
  add eax, esi    /// common weight
  add eax, edi    /// skew weight
  mov [rbp], eax  /// save change
  mov [rcx*4+rdx+50], eax

  test edi,edi
  je done /// change skew value?
  shr edi,1 /// div 2
done:
  inc ecx
  ret
  aligndq

lot_mod_code:
  call commonPreset
  mov dword ptr[rdx+0c], -1
  cmp byte ptr[rdx+03], 1 /// loopx8 for all drop?
  je @f
  readmem(lot_mod,6)
  jmp lot_mod_return
@@:
  push rdi
  push rsi
  push rbx
  push rcx
  push rdx
  sub rsp,20
  mov rdi,rdx
  mov rsi,rcx
  xor rbx,rbx
@@:
  mov rcx, drop_setup_data
  cmp ebx,[rcx+10]
  jae @f
  mov dword ptr[rdi+0c], ebx
  mov rcx, rsi
  call oriDrop
  inc rbx
  jmp @b
@@:
  add rsp,20
  pop rdx
  pop rcx
  pop rbx
  pop rsi
  pop rdi
oriDrop:
  readmem(lot_mod, 6)
  jmp lot_mod_return
  aligndq

drop_setup_data:
  dq 0 0 0 0 0 0 0

drop_mod+5:
  db 1
drop_mod_return:

rate_mod:
  jmp rate_mod_code  //  check nops
rate_mod_return:

lot_mod:
  jmp lot_mod_code  //  check nops
  nop
lot_mod_return:


[DISABLE]
drop_mod:
  readmem(drop_rate_99_with_weight_mod, 16)
rate_mod:
  readmem(drop_rate_99_with_weight_mod+10, 16)
lot_mod:
  readmem(drop_rate_99_with_weight_mod+20, 16)


